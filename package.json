{
  "name": "reactSSR",
  "version": "1.0.0",
  "description": "react server side rendering",
  "main": "index.js",
  "author": "lixf",
  "license": "MIT",
  "scripts": {
    "build:client": "webpack --config webpack.client.config.js --progress --colors",
    "start:client": "webpack-dev-server --config webpack.client.config.js",
    "start:server": "nodemon src/server/server.js --exec babel-node",
    "build:server1": "webpack --config webpack.server.config.js --progress --colors",
    "build:server": "babel src/client -d dist/client && babel src/server -d dist/server",
    "serve": "node dist/server/server.js"
  },
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.19",
    "@types/redux": "^3.6.0",
    "@types/webpack": "^3.8.1",
    "@types/webpack-dev-middleware": "^1.9.4",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.5"
  }
}
